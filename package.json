{
  "name": "google-maps-dashboard",
  "version": "1.0.0",
  "description": "Real-time Google Maps dashboard with heatmaps and route optimization",
  "main": "dist/server/index.js",
  "scripts": {
    "build": "tsc",
    "build:client": "webpack --config webpack.config.js",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "ts-node-dev --respawn server/index.ts",
    "dev:client": "webpack serve --config webpack.config.js --mode development",
    "start": "node dist/server/index.js",
    "init-db": "ts-node server/init-db.ts"
  },
  "dependencies": {
    "@googlemaps/js-api-loader": "^1.16.2",
    "@microsoft/signalr": "^8.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "sql.js": "^1.9.0",
    "uuid": "^9.0.0",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/google.maps": "^3.54.10",
    "@types/node": "^20.10.4",
    "@types/sql.js": "^1.4.9",
    "@types/uuid": "^9.0.7",
    "@types/ws": "^8.5.10",
    "@vercel/node": "^5.5.15",
    "concurrently": "^8.2.2",
    "css-loader": "^6.8.1",
    "html-webpack-plugin": "^5.5.4",
    "style-loader": "^3.3.3",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "keywords": [
    "google-maps",
    "heatmap",
    "signalr",
    "real-time",
    "dashboard"
  ],
  "author": "",
  "license": "MIT"
}
